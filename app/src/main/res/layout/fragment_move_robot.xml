    <EditText
        android:id="@+id/editText_Wrist2"
        android:layout_width="58dp"
        android:layout_height="34dp"
        android:layout_marginStart="136dp"
        android:layout_marginTop="16dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:textSize="10sp"
