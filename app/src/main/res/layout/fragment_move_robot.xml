    <Button
        android:id="@+id/btnBaseRight"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="16dp"
        android:onClick="click_MoveRight"
        android:tag="0:+"
        android:text="+"
        app:layout_constraintStart_toEndOf="@+id/editText_Base"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btnBaseLeft"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginEnd="10dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="16dp"
        android:onClick="click_MoveLeft"
        android:text="-"
        android:tag="0:-"
        app:layout_constraintEnd_toStartOf="@+id/editText_Base"
        app:layout_constraintStart_toEndOf="@+id/textView_Base"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
