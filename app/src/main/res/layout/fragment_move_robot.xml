<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <!--tools:context="com.automatizacion.fluxing.fluxingunivesalrobotsui.MoveRobotFragment"-->


    <SeekBar
        android:id="@+id/seekBar_Base"
        android:layout_width="207dp"
        android:layout_height="22dp"
        android:layout_marginStart="29dp"
        android:layout_marginTop="16dp"
        android:max="720"
        app:layout_constraintStart_toEndOf="@+id/btnBaseRight"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />

    <SeekBar
        android:id="@+id/seekBar_Shoulder"
        android:layout_width="207dp"
        android:layout_height="22dp"
        android:layout_marginStart="29dp"
        android:layout_marginTop="28dp"
        android:max="720"
        app:layout_constraintStart_toEndOf="@+id/btnShoulderRight"
        app:layout_constraintTop_toBottomOf="@+id/seekBar_Base"
        tools:ignore="MissingConstraints" />

    <SeekBar
        android:id="@+id/seekBar_Elbow"
        android:layout_width="207dp"
        android:layout_height="22dp"
        android:layout_marginStart="29dp"
        android:layout_marginTop="28dp"
        android:max="720"
        app:layout_constraintStart_toEndOf="@+id/btnElbowRight"
        app:layout_constraintTop_toBottomOf="@+id/seekBar_Shoulder"
        tools:ignore="MissingConstraints" />

    <SeekBar
        android:id="@+id/seekBar_Wrist1"
        android:layout_width="207dp"
        android:layout_height="22dp"
        android:layout_marginStart="29dp"
        android:layout_marginTop="28dp"
        android:max="720"
        app:layout_constraintStart_toEndOf="@+id/btnWrist1Right"
        app:layout_constraintTop_toBottomOf="@+id/seekBar_Elbow"
        tools:ignore="MissingConstraints" />

    <SeekBar
        android:id="@+id/seekBar_Wrist2"
        android:layout_width="207dp"
        android:layout_height="22dp"
        android:layout_marginStart="29dp"
        android:layout_marginTop="28dp"
        android:max="720"
        app:layout_constraintStart_toEndOf="@+id/btnWrist2Right"
        app:layout_constraintTop_toBottomOf="@+id/seekBar_Wrist1"
        tools:ignore="MissingConstraints" />

    <SeekBar
        android:id="@+id/seekBar_Wrist3"
        android:layout_width="207dp"
        android:layout_height="22dp"
        android:layout_marginStart="29dp"
        android:layout_marginTop="28dp"
        android:max="720"
        app:layout_constraintStart_toEndOf="@+id/btnWrist3Right"
        app:layout_constraintTop_toBottomOf="@+id/seekBar_Wrist2"
        tools:ignore="MissingConstraints" />

    <EditText
