<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <!--tools:context="com.automatizacion.fluxing.fluxingunivesalrobotsui.MoveRobotFragment"-->


    <SeekBar
        android:id="@+id/seekBar_Base"
        android:layout_width="207dp"
        android:layout_height="22dp"
        android:layout_marginLeft="92dp"
        android:layout_marginTop="16dp"
        android:max="720"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <SeekBar
        android:id="@+id/seekBar_Shoulder"
        android:layout_width="207dp"
        android:layout_height="22dp"
        android:layout_marginLeft="92dp"
        android:layout_marginTop="32dp"
        android:max="720"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/seekBar_Base" />

    <SeekBar
        android:id="@+id/seekBar_Elbow"
        android:layout_width="207dp"
        android:layout_height="22dp"
        android:layout_marginLeft="92dp"
        android:layout_marginTop="28dp"
        android:max="720"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/seekBar_Shoulder" />

    <SeekBar
        android:id="@+id/seekBar_Wrist1"
        android:layout_width="207dp"
        android:layout_height="22dp"
        android:layout_marginLeft="92dp"
        android:layout_marginTop="28dp"
        android:max="720"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/seekBar_Elbow" />

    <SeekBar
        android:id="@+id/seekBar_Wrist2"
        android:layout_width="207dp"
        android:layout_height="22dp"
        android:layout_marginLeft="92dp"
        android:layout_marginTop="28dp"
        android:max="720"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/seekBar_Wrist1" />

    <SeekBar
        android:id="@+id/seekBar_Wrist3"
        android:layout_width="207dp"
        android:layout_height="22dp"
        android:layout_marginLeft="92dp"
        android:layout_marginTop="28dp"
        android:max="720"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/seekBar_Wrist2" />

    <EditText
        android:id="@+id/editText_Base"
        android:layout_width="58dp"
        android:layout_height="34dp"
        android:layout_marginStart="60dp"
        android:layout_marginTop="10dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:textSize="10sp"
        app:layout_constraintStart_toEndOf="@+id/seekBar_Base"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/editText_Shoulder"
        android:layout_width="58dp"
        android:layout_height="34dp"
        android:layout_marginStart="60dp"
        android:layout_marginTop="16dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:textSize="10sp"
        app:layout_constraintStart_toEndOf="@+id/seekBar_Shoulder"
        app:layout_constraintTop_toBottomOf="@+id/editText_Base" />

    <EditText
        android:id="@+id/editText_Elbow"
        android:layout_width="58dp"
        android:layout_height="34dp"
        android:layout_marginStart="60dp"
        android:layout_marginTop="16dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:textSize="10sp"
        app:layout_constraintStart_toEndOf="@+id/seekBar_Elbow"
        app:layout_constraintTop_toBottomOf="@+id/editText_Shoulder" />

    <EditText
        android:id="@+id/editText_Wrist1"
        android:layout_width="58dp"
        android:layout_height="34dp"
        android:layout_marginStart="60dp"
        android:layout_marginTop="16dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:textSize="10sp"
        app:layout_constraintStart_toEndOf="@+id/seekBar_Wrist1"
        app:layout_constraintTop_toBottomOf="@+id/editText_Elbow" />

    <EditText
        android:id="@+id/editText_Wrist2"
        android:layout_width="58dp"
        android:layout_height="34dp"
        android:layout_marginLeft="60dp"
        android:layout_marginTop="16dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:textSize="10sp"
        app:layout_constraintStart_toEndOf="@+id/seekBar_Wrist2"
        app:layout_constraintTop_toBottomOf="@+id/editText_Wrist1" />

    <EditText
        android:id="@+id/editText_Wrist3"
        android:layout_width="58dp"
        android:layout_height="34dp"
        android:layout_marginLeft="60dp"
        android:layout_marginTop="16dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:textSize="10sp"
        app:layout_constraintStart_toEndOf="@+id/seekBar_Wrist3"
        app:layout_constraintTop_toBottomOf="@+id/editText_Wrist2" />

    <Button
        android:id="@+id/button_FreeDrive"
        android:layout_width="106dp"
        android:layout_height="92dp"
        android:layout_marginRight="72dp"
        android:layout_marginTop="24dp"
        android:text="Freedrive"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button_HomePosition"
        android:layout_width="106dp"
        android:layout_height="92dp"
        android:layout_marginRight="72dp"
        android:layout_marginTop="136dp"
        android:text="Home"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView_Base"
        android:layout_width="wrap_content"
        android:layout_height="20dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:text="Base"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView_Shoulder"
        android:layout_width="wrap_content"
        android:layout_height="20dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="33dp"
        android:text="Shoulder"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView_Base" />

    <TextView
        android:id="@+id/textView_Elbow"
        android:layout_width="wrap_content"
        android:layout_height="20dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="31dp"
        android:text="Elbow"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView_Shoulder" />

    <TextView
        android:id="@+id/textView_Wrist1"
        android:layout_width="wrap_content"
        android:layout_height="20dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="30dp"
        android:text="Wrist1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView_Elbow" />

    <TextView
        android:id="@+id/textView_Wrist2"
        android:layout_width="wrap_content"
        android:layout_height="20dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="31dp"
        android:text="Wrist2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView_Wrist1" />

    <TextView
        android:id="@+id/textView_Wrist3"
        android:layout_width="wrap_content"
        android:layout_height="20dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="30dp"
        android:text="Wrist3"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView_Wrist2" />

</android.support.constraint.ConstraintLayout>
